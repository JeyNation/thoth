[
  { "name": "number sequence", "regex": "\\b(\\d+)\\b" },
  { "name": "positive integer", "regex": "\\b([1-9]\\d*)\\b" },
  { "name": "integer (signed)", "regex": "\\b([+-]?\\d+)\\b" },
  { "name": "decimal number", "regex": "\\b([+-]?\\d+(?:\\.\\d+)?)\\b" },
  { "name": "currency amount (north american)", "regex": "\\b(\\d{1,3}(?:[,\\s]?\\d{3})*(?:\\.\\d+)?)\\b" },
  { "name": "currency amount (euro)", "regex": "\\b(\\d{1,3}(?:[.\\s]?\\d{3})*(?:,\\d+)?)\\b" },
  { "name": "alphanumeric token", "regex": "\\b([A-Za-z0-9]+)\\b" },
  { "name": "alphabetic word", "regex": "\\b([A-Za-z]+)\\b" },
  { "name": "email address", "regex": "\\b([A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,})\\b" },
  { "name": "url (http/https)", "regex": "\\b((?:https?):\\/\\/[\\w.-]+(?::[0-9]+)?(?:\\/[\\w\\-./?%&=]*)?)\\b" },
  { "name": "date (yyyy-mm-dd)", "regex": "\\b(\\d{4}-\\d{2}-\\d{2})\\b" },
  { "name": "date (mm/dd/yyyy)", "regex": "\\b((?:0?[1-9]|1[0-2])\\/(?:0?[1-9]|[12]\\d|3[01])\\/\\d{4})\\b" },
  { "name": "time (hh:mm)", "regex": "\\b((?:[01]?\\d|2[0-3]):[0-5]\\d)\\b" },
  { "name": "iso 8601 datetime", "regex": "\\b(\\d{4}-\\d{2}-\\d{2}[T\\s]\\d{2}:\\d{2}(?::\\d{2})?(?:Z|[+-]\\d{2}:?\\d{2})?)\\b" },
  { "name": "us zip code", "regex": "\\b(\\d{5}(?:-\\d{4})?)\\b" },
  { "name": "phone (us)", "regex": "\\b((?:\\+1[\\s-]?)?(?:\\(?\\d{3}\\)?[\\s-]?)?\\d{3}[\\s-]?\\d{4})\\b" },
  { "name": "currency amount", "regex": "\\b(\\$?\\d{1,3}(?:,\\d{3})*(?:\\.\\d{2})?)\\b" },
  { "name": "uuid v4", "regex": "\\b([0-9a-fA-F]{8}-[0-9a-fA-F]{4}-4[0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12})\\b" },
  { "name": "hex color", "regex": "\\b(#(?:[0-9a-fA-F]{3}){1,2})\\b" },
  { "name": "non-whitespace sequence", "regex": "\\b(\\S+)\\b" }
]
