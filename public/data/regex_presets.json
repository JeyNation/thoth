[
  { "name": "number sequence", "regex": "(\\d+)" },
  { "name": "positive integer", "regex": "([1-9]\\d*)" },
  { "name": "integer (signed)", "regex": "([+-]?\\d+)" },
  { "name": "decimal number", "regex": "([+-]?\\d+(?:\\.\\d+)?)" },
  { "name": "alphanumeric token", "regex": "([A-Za-z0-9]+)" },
  { "name": "alphabetic word", "regex": "([A-Za-z]+)" },
  { "name": "email address", "regex": "([A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,})" },
  { "name": "url (http/https)", "regex": "((?:https?):\\/\\/[\\w.-]+(?::[0-9]+)?(?:\\/[\\w\\-./?%&=]*)?)" },
  { "name": "date (yyyy-mm-dd)", "regex": "(\\d{4}-\\d{2}-\\d{2})" },
  { "name": "date (mm/dd/yyyy)", "regex": "((?:0?[1-9]|1[0-2])\\/(?:0?[1-9]|[12]\\d|3[01])\\/\\d{4})" },
  { "name": "time (hh:mm)", "regex": "((?:[01]?\\d|2[0-3]):[0-5]\\d)" },
  { "name": "iso 8601 datetime", "regex": "(\\d{4}-\\d{2}-\\d{2}[T\\s]\\d{2}:\\d{2}(?::\\d{2})?(?:Z|[+-]\\d{2}:?\\d{2})?)" },
  { "name": "us zip code", "regex": "(\\d{5}(?:-\\d{4})?)" },
  { "name": "phone (us)", "regex": "((?:\\+1[\\s-]?)?(?:\\(?\\d{3}\\)?[\\s-]?)?\\d{3}[\\s-]?\\d{4})" },
  { "name": "currency amount", "regex": "(\\$?\\d{1,3}(?:,\\d{3})*(?:\\.\\d{2})?)" },
  { "name": "uuid v4", "regex": "([0-9a-fA-F]{8}-[0-9a-fA-F]{4}-4[0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12})" },
  { "name": "hex color", "regex": "(#(?:[0-9a-fA-F]{3}){1,2})" },
  { "name": "non-whitespace sequence", "regex": "(\\\\S+)" },
  { "name": "invoice number (letters+digits)", "regex": "([A-Za-z]{2,}[\\- ]?\\d{2,})" }
]
